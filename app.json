{
  "expo": {
    "name": "Vily",
    "slug": "PortfolioVilyx",
    "scheme": "PortfolioVilyx", 
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/vilyperfil.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/vilyperfil.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/vilyperfil.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "web": {
      "favicon": "./assets/images/vilyperfil.png"
    }
  }
}